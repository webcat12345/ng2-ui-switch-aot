{
  "name": "ngx-ui-switch",
  "version": "10.0.2",
  "description": "switch button for angular4",
  "bugs": "https://github.com/webcat12345/ngx-ui-switch/issues",
  "homepage": "https://github.com/webcat12345/ngx-ui-switch#readme",
  "repository": "https://github.com/webcat12345/ngx-ui-switch.git",
  "license": "MIT",
  "author": "webcat12345",
  "private": true,
  "keywords": [
    "angular 2",
    "angular 4",
    "angular 5",
    "angular 6",
    "angular 7",
    "ngx",
    "ng2",
    "uiswitch",
    "switch",
    "ui"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "rimraf dist && ts-node -P tsconfig.build.json build.ts",
    "build:demo": "rimraf demo && ng build --aot --prod --base-href=/ngx-ui-switch/demo/ --output-path=demo",
    "release": "cd dist/packages-dist && yarn publish",
    "bundlewatch": "bundlewatch",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "cleanup": "rimraf build dist waste deploy demo docs",
    "lint": "ng lint",
    "lint:ngx-ui-switch": "ng lint ngx-ui-switch --type-check",
    "e2e": "ng e2e",
    "e2e:ci": "ng e2e --protractor-config=protractor-ci.conf.js",
    "typedoc": "typedoc --options typedoc.json src/lib/*.ts && ts-node -P tsconfig.build.json docs.ts"
  },
  "dependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/common": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.1",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@angular/language-service": "^10.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "all-contributors-cli": "^5.10.0",
    "bundlewatch": "^0.2.5",
    "codelyzer": "^6.0.0",
    "copy": "^0.3.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^5.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.1",
    "rollup-plugin-filesize": "^1.4.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "~8.10.0",
    "tsickle": "^0.39.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.14.2",
    "typescript": "~3.9.5"
  },
  "angular-cli": {},
  "bundlewatch": [
    {
      "path": "./dist/packages-dist/ui-switch.umd.js",
      "maxSize": "8 kB"
    }
  ]
}
